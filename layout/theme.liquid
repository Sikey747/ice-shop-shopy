<!doctype html>
<html>
  <head>
    <title>{{ page_title }}</title>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="description" content="{{ page_description | escape }}">
    <link rel="canonical" href="{{ canonical_url }}">
    <meta name="viewport" content="width=device-width,initial-scale=1,shrink-to-fit=no">
  {{ content_for_header }}
    <!-- Header hook for plugins -->
    {{ 'application.css' | asset_url | stylesheet_tag }}
    <script defer src="{{ 'application.js' | asset_url }}"></script>
    <script defer src="{{ 'aos.min.js' | asset_url }}"></script>
    <script defer src="{{ 'jquery.js' | asset_url }}"></script>
    <script defer src="{{ 'scrollLock.js' | asset_url }}"></script>
    <script defer src="{{ 'toastify.js' | asset_url }}"></script>

    {{ "aos.min.css" | asset_url | stylesheet_tag }}
    {{ "toastify.css" | asset_url | stylesheet_tag }}

    {% style %}:root
    {
      --primary-black: #282828;
      --primary-white: {{ settings.bg-color }};
      --primary-beaver: #79987;
      --primary-gray1: #5F6980;
      --primary-gray2: #D0D5DD;
      --primary-gray3: #9D9FA2;
      --container-with: 1310px;
      --container-padding: 15px;
      --tw-ring-color: var(--primary-black);
      --f1: "Inter";
      --accordion-btn-active-icon: url("{{ "chevron.svg" | asset_url }}");
      --bs-accordion-button-active-icon: url("data:image/svg+xml, <svg xmlns='http://www.w3.org/2000/svg'");

    }

    .shadow1 {
      box-shadow: 0 1px 10px 0 rgba(0, 0, 0, 0.06);
    }

    body {
      background-color: var(--primary-white);
      color: var(--primary-black);
      margin: 0 auto;
      font-family: var(--f1);
      overflow-x: hidden;
    }
    .container {
      max-width: 1370px;
      padding-left: var(--container-padding);
      padding-right: var(--container-padding);
      margin: 0 auto;
    }

  {% endstyle %}

</head>
<body>
  {% section 'header' %}
  <main role="main">
    {{ content_for_layout }}
  </main>
  {% section 'footer' %}
</body></html><script defer>
  window.addEventListener("DOMContentLoaded",()=>{
      AOS.init();
  })

</script>